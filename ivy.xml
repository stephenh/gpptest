<ivy-module version="2.0">
	<info organisation="gpptest" module="gpptest" revision="${version}"/>
	<configurations>
		<conf name="default" extends="compile"/>
		<conf name="compile" visibility="private"/>
		<conf name="test" visibility="private"/>
		<conf name="provided" visibility="private"/>
		<conf name="sources"/>
	</configurations>
	<dependencies defaultconfmapping="%->default;sources->sources()">
		<!--
		<dependency org="org.codehaus.groovy" name="groovy" rev="1.8.0-rc-1" conf="compile,sources"/>
		-->
		<dependency org="org.mbte.groovypp" name="groovypp" rev="0.4.155" conf="compile,sources"/>
	</dependencies>
</ivy-module>
